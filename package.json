{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "dockerClean": "docker stop $(docker ps -a -q) && docker rm $(docker ps -a -q)",
    "docker": "tsc && docker-compose up --build",
    "start": "tsc && node ./dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts --watch src",
    "test": "jest --runInBand --ci --forceExit",
    "coverage": "jest --coverage --runInBand --forceExit",
    "coveralls": "jest --coverage --runInBand  --forceExit && cat ./coverage/lcov.info | coveralls ",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "watch-node": "nodemon dist/server.js",
    "watch": "npm run generate && concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "prod": "yarn --production && export NODE_ENV=production && yarn build && yarn start",
    "prod-windows": "yarn --production && set NODE_ENV=production && yarn build && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicky/api.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dicky/api/issues"
  },
  "homepage": "https://github.com/dicky/api#readme",
  "devDependencies": {
    "@hapi/joi": "^15.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/hapi__joi": "^15.0.3",
    "@types/helmet": "0.0.43",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "jest": "^24.8.0",
    "mongo-unit": "^1.4.5",
    "nodemon": "^1.19.1",
    "socket.io-client": "^2.3.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
