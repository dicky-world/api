    "@types/agenda": "^2.0.6",
    "@types/authenticator": "^1.1.0",
    "@types/node-fetch": "^2.5.3",
    "@types/sha1": "^1.1.2",
    "@types/supertest": "^2.0.8",
    "agenda": "^2.1.0",
    "authenticator": "^1.1.5",
    "aws-sdk": "^2.528.0",
    "bcrypt": "^3.0.6",
